#!/bin/bash

set -e

branch=`echo $1 | awk '{print tolower($0)}'`
if [ "${branch}" = "develop" ]; then
  echo develop
elif [ "${branch}" = "master" ]; then
  echo latest
elif [ "${branch}" = "chores/explorer-tags-for-dockerhub" ]; then
  echo chores/explorer-tags-for-dockerhub
else
  echo unable to deduce docker tag from ${branch}
  exit 1
fi
